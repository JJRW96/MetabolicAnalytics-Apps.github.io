file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/Watts2023_Demarcation of Intensity From 3 to 5 Zones Aids in.pdf:pdf},
issn = {15334287},
journal = {Journal of Strength and Conditioning Research},
keywords = {ergometer time-trial,intensity distribution,lactate threshold,on-water race,peak oxygen consumption},
number = {11},
pages = {E593--E600},
