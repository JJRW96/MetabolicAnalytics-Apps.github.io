issn = {1600-0838 (Electronic)},
journal = {Scandinavian journal of medicine & science in sports},
keywords = {Erythrocyte Volume,Hemoglobins,Hot Temperature,Humans,Male,Oxygen Consumption,Physical Endurance,Running,analysis,physiology},
