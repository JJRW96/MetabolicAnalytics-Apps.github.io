journal = {International Journal of Sports Physiology and Performance},
keywords = {and,any sport involve correct,c times in the,cycling,endurance,exercise,intensity,manipulation of the training,performances at speci fi,prescription,successful training programs in,variables frequency,volume to achieve peak},
pages = {1--4},
title = {{What Is “Zone 2 Training”?: Experts' Viewpoint on Definition, Training Methods, and Expected Adaptations}},
url = {https://journals.humankinetics.com/view/journals/ijspp/aop/article-10.1123-ijspp.2024-0303/article-10.1123-ijspp.2024-0303.xml},
