keywords = {Cycling,Endurance,Physiology,Power meter},
number = {9},
pages = {783--786},
pmid = {35835698},
publisher = {The Author(s)},
title = {{Time to exhaustion at estimated functional threshold power in road cyclists of different performance levels}},
