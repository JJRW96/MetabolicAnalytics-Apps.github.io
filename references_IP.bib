journal = {International Journal of Sports Physiology and Performance},
keywords = {and technological advances has,athlete biological passport,cant climbs of the,cycling,elite cycling is continuously,endurance performance,evolving,nutritional,power meter,records being broken on,resulted in,the combination of,the most signi fi,training},
pages = {1--4},
title = {{Antidoping 2.0: Is Adding Power-Output Data to the Antidoping Pool the Next Step? Experts' Viewpoint}},
