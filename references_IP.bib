file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/Leo2025_Mechanistic influence of the torque cadence relationship on power output during exhaustive all-out field tests in professional cyclists.pdf:pdf},
issn = {1466447X},
journal = {Journal of Sports Sciences},
keywords = {Cycling,high performance,sprinting,testing,training},
number = {9},
pages = {887--894},
publisher = {Routledge},
title = {{Mechanistic influence of the torque cadence relationship on power output during exhaustive all-out field tests in professional cyclists}},
url = {https://doi.org/10.1080/02640414.2025.2478354},
volume = {43},
