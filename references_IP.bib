issn = {1664042X},
journal = {Frontiers in Physiology},
keywords = {cycling efficiency,cycling time trial,exercise,maximal oxygen uptake,peak power output},
number = {November},
pages = {1--9},
title = {{Prolonged Heat Acclimation and Aerobic Performance in Endurance Trained Athletes}},
volume = {10},
year = {2019}
}
