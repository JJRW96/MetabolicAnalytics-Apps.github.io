issn = {01931849},
journal = {American Journal of Physiology - Endocrinology and Metabolism},
keywords = {Free fatty acids,Glucose transporters,Glycogen,Lactate,Leg blood flow,Oxygen uptake,Phosphorylation potential,Pyruvate dehydrogenase},
number = {1 38-1},
