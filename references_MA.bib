issn = {0161-7567 (Print)},
journal = {Journal of applied physiology: respiratory, environmental and exercise physiology},
keywords = {Air,Altitude,Humans,Mathematics,Motion,Oxygen Consumption,Posture,Sports Medicine,Temperature},
language = {eng},
