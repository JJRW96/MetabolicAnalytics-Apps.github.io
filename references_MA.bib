journal = {European Journal of Applied Physiology},
keywords = {Comparison,Creatine phosphate,Lactate,Oxygen uptake,Testing device},
number = {6},
pages = {748--751},
pmid = {16468058},
title = {{Mechanically versus electro-magnetically braked cycle ergometer: Performance and energy cost of the Wingate Anaerobic Test}},
