@article{Binzoni1991,
abstract = {A mathematical model is developed whereby the longitudinal magnetization of phosphocreatine (PC), ATP, P(i), and total phosphate (P(T)) can be calculated on the basis of assumed chemical rate constants (k(i)) and spin lattice relaxation times of the muscle PC ⇆ ATP ⇆ P(i) exchange system. By means of this model, some unexplained 31P nuclear magnetic resonance (NMR) spectroscopy results from the literature (e.g., a decrease of P(T) in a closed system) could be explained simply on the basis of the physiological variability of k(i). Moreover, appropriate model simulations indicate that 1) 31P-NMR spectra obtained with short relaxation delays may be influenced to various extents by the metabolic and physicochemical status of the muscle; 2) the assessment of k(i) by standard NMR spectroscopy techniques may be extremely critical; 3) $\delta$PC/$\delta$P(i), as obtained from conventional 31P-NMR spectra, may represent a sensible index of k2 (the pseudo first-order chemical exchange rate constant of the adenosinetriphosphatase reaction); 4) $\delta$PC/$\delta$P(i) changes as detected from sequential (short relaxation delays) 31P-NMR spectra obtained in humans during metabolic transients (e.g., during transition from rest to work and vice versa) may represent an index of transient changes of k2.},
author = {Binzoni, T. and Cerretelli, P.},
doi = {10.1152/jappl.1991.71.5.1700},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Binzoni, Cerretelli - 1991 - Muscle 31P-NMR in humans Estimate of bias and qualitative assessment of ATPase activity.pdf:pdf},
issn = {01617567},
journal = {Journal of Applied Physiology},
keywords = {chemical rate constants,magnetization transfer,muscle high-energy phosphates,spectra simulation},
number = {5},
pages = {1700--1704},
pmid = {1837013},
title = {{Muscle 31P-NMR in humans: Estimate of bias and qualitative assessment of ATPase activity}},
volume = {71},
year = {1991}
}
