file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Meyer et al. - 2001 - Reliability of gas exchange measurements from two different spiroergometry systems.pdf:pdf},
issn = {01724622},
journal = {International Journal of Sports Medicine},
keywords = {Bland-Altman,Gas exchange,Reliability coefficient,Reproducibility},
number = {8},
pages = {593--597},
pmid = {11719895},
title = {{Reliability of gas exchange measurements from two different spiroergometry systems}},
volume = {22},
