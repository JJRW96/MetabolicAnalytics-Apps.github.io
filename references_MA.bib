@article{DiPrampero1990,
abstract = {The factors limiting V̇O2 max in humans are analyzed according to a multigfactorial model derived from the O2 conductance equation. In this context, alveolar ventilation (V̇a) and lung O2 transfer (Gl) are not considered to be limiting, at least at sea level, because changes in V̇a and/or in Gl are not accompanied by changes in V̇O2max due to the shape of the O2 dissociation curve. Thus, the limits of V̇O2max are shared between blood O2 transport (Fq′) and a peripheral factor. This last includes tissue O2 transfer (Ft′) and mitochondrial O2 utilization (Fm′). In untrained subjects at sea level, blood O2 transport is found to be responsible for ∼70% of the overall to limits to V̇O2max (Fq′ = 0.7), the rest depending on the peripheral factor. Fq′, a well as the sum of Ft′ and Fm′, are unchanged after training or upon return to sea level following exposure to chronic hypoxia (altitude higher than 5000 m) In the latter condition, however, since tissue O2 transfer, which sea sets Ft′ is facilitated, and mitochondrial O2 utilization, which sets Fm′, is impaired, Ft′ is reduced and Fm′ increased as compared to control condition and/or after training. {\textcopyright} 1990.},
author = {di Prampero, Pietro Enrico and Ferretti, Guido},
doi = {10.1016/0034-5687(90)90075-A},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/di Prampero, Ferretti - 1990 - Factors limiting maximal oxygen consumption in humans.pdf:pdf},
issn = {00345687},
journal = {Respiration Physiology},
keywords = {Alveolar ventilation,Animal, man,Blood, O2 dissociation curve,Cardiac outpu, maximal,Diffusing capacity, palmonary,Gas exchange,Hemoglobin,Muscle morphometry,O2 consumption, maximal},
number = {2-3},
pages = {113--128},
pmid = {2218094},
title = {{Factors limiting maximal oxygen consumption in humans}},
volume = {80},
year = {1990}
}
@article{DiPrampero1983,
abstract = {Single-breath O2 consumption (V̇O2) at the mouth and heart rate were determined in 5 healthy male subjects at rest, during 8 min of cycloergometric exercise (50, 100, 125, and 150 W), and in the recovery period following two experimental conditions: air breathing throughout (AA); hypoxic breathing (FI(O2) = 0.11) for 6 min of preexercise rest followed by air breathing from the onset of exercise (HA). The O2 deficits and debts as well as the t( 1/2 ) values of the V̇O2 on- and off-responses were determined and blood lactate concentrations measured at rest and in the recovery after 4 and 8 min of exercise. At all work loads: 1) O2 deficits were on the average 0.39 liter smaller in HA than in AA; 2) V̇O2 on-responses were faster in HA (t( 1/2 ) ≃ 7 s) than in AA (t( 1/2 ) = 20-30 s); and 3) O2 debts and V̇O2 off-responses were the same in the two conditions. Since the V̇O2 and heart rate levels at steady state as well as the blood lactate concentrations after 4 and 8 min of exercise were the same in AA and HA, the observed differences of O2 deficit cannot be attributed to changes of energy metabolism in the two conditions; they therefore depend on the reduction of body O2 stores at rest in HA. This, independently measured, was found to be 0.46 liters, not far from the observed O2 deficit difference (0.39 liters). Thus a decrease of O2 stores before exercise is accompanied by a reduction of the O2 deficit and faster V̇O2 kinetics at the onset of exercise.},
author = {{Di Prampero}, P. E. and Boutellier, U. and Pietsch, P.},
doi = {10.1152/jappl.1983.55.1.146},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Di Prampero, Boutellier, Pietsch - 1983 - Oxygen deficit and stores at onset of muscular exercise in humans.pdf:pdf},
issn = {01617567},
journal = {Journal of Applied Physiology Respiratory Environmental and Exercise Physiology},
number = {1 I},
pages = {146--153},
pmid = {6885564},
title = {{Oxygen deficit and stores at onset of muscular exercise in humans}},
volume = {55},
year = {1983}
}
