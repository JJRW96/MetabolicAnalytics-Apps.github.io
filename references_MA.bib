issn = {0195-9131},
journal = {Medicine & Science in Sports & Exercise},
keywords = {CLIMBING,DEAD CENTER SIZE,EMG,MUSCLE ACTIVITY,PEDAL FORCES,UPHILL CYCLING},
number = {5},
