issn = {0021-9290},
journal = {Journal of biomechanics},
keywords = {Adolescent,Adult,Bicycling,Energy Metabolism,Exercise Test,Humans,Locomotion,Oxygen Consumption,Sitting Position,Standing Position,Young Adult,physiology},
