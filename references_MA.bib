journal = {European Journal of Applied Physiology},
keywords = {Adolescent,Adult,Capnography,Carbon Dioxide,Equipment Design,Equipment Failure Analysis,Humans,Male,Metamax,Miniaturization,Oximetry,Oxygen,Portable gas analysis,Pulmonary Gas Exchange,Reliability,Reproducibility of Results,Sensitivity and Specificity,Spirometry,Stability,Validity,analysis,instrumentation,physiology},
