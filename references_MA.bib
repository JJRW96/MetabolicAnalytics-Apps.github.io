@article{Ferguson2002,
abstract = {The effect of elevated human muscle temperature on energy turnover was investigated during cycling exercise (at 85% of V̇O2max) at a contraction frequency of 60 revs min-1. Muscle temperature was passively elevated prior to exercise by immersion of the legs in a hot water bath (42°C). During exercise at this low pedalling rate, total energy turnover was higher (P<0.05) when muscle temperature was elevated compared with normal temperature (70.4±3.7 versus 66.9±2.4 kJ min-1, respectively). Estimated net mechanical efficiency was found to be lower when muscle temperature was elevated. A second experiment was conducted in which the effect of elevated human muscle temperature on energy turnover was investigated during cycling exercise (at 85% of V̇O2max) at a contraction frequency of 120 revs min-1. Under the conditions of a high pedalling frequency, an elevated muscle temperature resulted in a lower energy turnover (P<0.05) compared with the normal muscle temperature (64.9±3.7 versus 69.0±4.7 kJ min-1, respectively). The estimated net mechanical efficiency was therefore higher when muscle temperature was elevated. We propose that, in these experiments, prior heating results in an inappropriately fast rate of cross-bridge cycling when exercising at 60 revs min-1, leading to an increased energy turnover and decreased efficiency. However, at the faster pedalling rate, the effect of heating the muscle shifts the efficiency/velocity relationship to the right so that cross-bridge detachment is more appropriately matched to the contraction velocity and, hence, energy turnover is reduced.},
author = {Ferguson, Richard A. and Ball, Derek and Sargeant, Anthony J.},
doi = {10.1242/jeb.205.7.981},
file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/Ferguson2002_Effect of muscle temperature on rate of oxygen uptake during exercise in humans at different contraction frequencies.pdf:pdf},
issn = {00220949},
journal = {Journal of Experimental Biology},
keywords = {Contraction velocity,Cycling exercise,Efficiency/velocity relationship,Human,Temperature},
number = {7},
pages = {981--987},
pmid = {11916993},
title = {{Effect of muscle temperature on rate of oxygen uptake during exercise in humans at different contraction frequencies}},
volume = {205},
year = {2002}
}
@article{Bolourchi1985,
abstract = {A computer-based instrumentation system which accurately measures the seat, handlebar, and pedal loads together with the absolute pedal position during bicycling is presented. The instrumentation system is unique in that for the first time it allows measurement of handlebar and seat loads. With three test subjects riding on rollers, which simulate actual bicycling, loading data were recorded at constant power for different pedaling rates. Analysis of the data enables load characteristics of all three sources (i.e., pedal, seat, and handlebar) to be categorized as being either subject and pedaling rate independent or subject independent but pedaling rate dependent. A complete set of loading data for one subject is presented to illustrate these characteristics. One interesting finding is that peak levels of all but one of the pedal loads are inversely related to pedaling rate. Peak levels of seat and handlebar loads reactive to the driving loads on the pedals, however, are directly related to pedaling rate. Another interesting result is that handlebar and seat loads, except for the lateral seat moment, go through two cycles for each revolution of the crank arm. The interpretation of these results, as well as others, provides insight into the biomechanics of the pedaling process.},
author = {Bolourchi, Farhad and Hull, M.L.},
doi = {10.1123/ijsb.1.4.308},
file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/Bolourchi_1985_Measurement of Rider Induced Loads during Simulated Bicycling.pdf:pdf},
issn = {0740-2082},
journal = {International Journal of Sport Biomechanics},
pages = {308--329},
title = {{Measurement of Rider Induced Loads during Simulated Bicycling}},
volume = {1},
year = {1985}
}
@article{Stone1993,
abstract = {This paper provides measurements of rider-induced loads during standing cycling. Two strain gauge dynamometers were used to measure these loads while three subjects rode bicycles on a large motorized treadmill; the cycling situation simulated hill climbing while standing. Comparing the results to those previously published for seated cycling revealed that the loading for standing cycling differed fundamentally from that for seated cycling in certain key respects. One respect was that the maximum magnitude normal pedal force reached substantially higher values, exceeding the weight of the subject, and the phase occurred later in the crank cycle. Another respect was that the direction of the handlebar forces alternated indicating that the arms pulled up and back during the power stroke of the corresponding leg and pushed down and forward during the upstroke. Inasmuch as these forces were coordinated (i.e., in phase) with the leaning of the bicycle, the arms developed positive power.},
author = {Stone, Cal and Hull, Maury L.},
doi = {10.1123/jab.9.3.202},
file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/Stone1993_RiderBicycle Interaction Loads during Standing Treadmill Cycling_ocred.pdf:pdf},
issn = {1065-8483},
journal = {Journal of Applied Biomechanics},
pages = {202--218},
title = {{Rider/Bicycle Interaction Loads during Standing Treadmill Cycling}},
volume = {9},
year = {1993}
