keywords = {absorption,endurance performance,exercise metabolism,exogenous carbohydrate oxidation,prolonged exercise},
number = {4},
pages = {452--457},
pmid = {20574242},
title = {{Carbohydrate and exercise performance: The role of multiple transportable carbohydrates}},
volume = {13},
year = {2010}
