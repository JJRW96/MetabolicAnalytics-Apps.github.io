issn = {10648011},
journal = {Journal of Strength and Conditioning Research},
keywords = {Exercise,Oxygen uptake,Validation},
number = {2},
pages = {344--348},
pmid = {15903373},
title = {{Reliability of the lode excalibur sport ergometer and applicability to computrainer electromagnetically braked cycling training device}},
volume = {19},
