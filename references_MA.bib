@article{McCann1995a,
abstract = {System linearity was assessed for exercise induced changes in energetics of forearm exercise. 31P-NMR spectroscopy techniques, with 12.5-s serial measurements of [PCr], [Pi], [ATP], and [H+] were employed during exercise and recovery transitions in four untrained men for moderate (1.7 W) and heavy (3.6 W) exercise. Signal averaging was applied and data were analyzed by regression analysis using a first-order exponential model. The time constants for both [PCr] and [Pi] responses to moderate exercise and recovery were not different both within and between nuclei ranging from 32 to 35 s (P > 0.05). The time constants derived from moderate exercise and recovery, when employed to construct predictive equations for heavy exercise and recovery, did not adequately describe [PCr] dynamics. Underestimation of the net hydrolysis of PCr during heavy exercise was associated with increases in [H+] as predicted by the creatine kinase equilibrium reaction (CKeq). Calculation of [ADP] by CKeq revealed steady state [ADP] was achieved during moderate exercise and during recovery for both intensities much earlier than during heavy exercise. We conclude that the metabolic system does not behave as a linear system. Therefore, the time constant and the net change in [PCr].W-1 must themselves be determined by work dependent combinations of other system variables.},
author = {McCann, D J and Mol{\'{e}}, P A and Caton, J R},
file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/McCann1995_PCr kinetics in human exercise and recovery_ocred.pdf:pdf;:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/McCann1995_PCr kinetics in human exercise and recovery_ocred.pdf:pdf},
issn = {0195-9131 (Print)},
journal = {Medicine and science in sports and exercise},
keywords = {Adenosine Triphosphate,Adult,Creatine Kinase,Ergometry,Forearm,Humans,Hydrogen,Hydrolysis,Kinetics,Magnetic Resonance Spectroscopy,Male,Muscle,Muscle Contraction,Phosphates,Phosphocreatine,Phosphorus Isotopes,Physical Exertion,Regression Analysis,Rest,Skeletal,instrumentation,metabolism,physiology},
language = {eng},
month = {mar},
number = {3},
pages = {378--389},
pmid = {7752865},
title = {{Phosphocreatine kinetics in humans during exercise and recovery.}},
volume = {27},
year = {1995}
}
