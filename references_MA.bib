file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jacobs et al. - 2006 - Endurance training has little effect on active muscle free fatty acid, lipoprotein cholesterol, or triglyceride n.pdf:pdf},
issn = {01931849},
journal = {American Journal of Physiology - Endocrinology and Metabolism},
keywords = {Cholesterol,Crossover concept,Exertion,Lipid metabolism},
