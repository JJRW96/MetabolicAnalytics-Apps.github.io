@article{Barstow1991,
abstract = {We assessed the linearity of oxygen uptake (V$\sim$2) kinetics for several work intensities in four trained cyclists. V$\sim$2 was measured breath by breath during transitions from 33 W (baseline) to work rates requiring 38, 54, 85, and 100% of maximal aerobic capacity (V$\sim$(2max)). Each subject repeated each work rate four times over 8 test days. In every case, three phases (phases 1, 2, and 3) of the V$\sim$2 response could be identified. V$\sim$2 during phase 2 was fit by one of two models: model 1, a double exponential where both terms begin together close to the start of phase 2, and model 2, a double exponential where each of the exponential terms begins independently with separate time delays. V$\sim$2 rose linearly for the two lower work rates (slope 11 ml {\textperiodcentered} min-1 W-1) but increased to a greater asymptote for the two heavier work rates. In all four subjects, for the two lighter work rates the double-exponential regression reduced to a single value for the time constant (average across subjects 16.1 ± 7.7 s), indicating a truly monoexponential response. In addition, one of the responses to the heaviest work rate was monoexponential. For the remaining seven biexponential responses to the two heaviest work rates, model 2 produced a significantly better fit to the responses (P < 0.05), with a mean time delay for the slow component of 105 ± 46 s. The amplitude of the primary component (A1) increased linearly across all four work rates (slope 9.1 ml {\textperiodcentered} min-1 {\textperiodcentered} W-1, r = 0.957). In contrast, the time constant for the fast component ($\tau$1) exhibited no significant change as work rate increased. Thus, although the overall oxygen cost increases nonlinearly with work intensity, the primary component of the V$\sim$2 response exhibits approximately linear behavior: linear increase in gain (A1) and invariant time constant ($\tau$1).},
author = {Barstow, T. J. and Mole, P. A.},
doi = {10.1152/jappl.1991.71.6.2099},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Barstow, Mole - 1991 - Linear and nonlinear characteristics of oxygen uptake kinetics during heavy exercise.pdf:pdf},
issn = {01617567},
journal = {Journal of Applied Physiology},
keywords = {energetics,modeling},
number = {6},
pages = {2099--2106},
pmid = {1778898},
title = {{Linear and nonlinear characteristics of oxygen uptake kinetics during heavy exercise}},
volume = {71},
year = {1991}
}
@article{Lamarra1987,
abstract = {Breathing has inherent irregularities that produce breath-to-breath fluctuations ('noise') in pulmonary gas exchange. These impair the precision of characterizing non-steady-state gas exchange kinetics during exercise. We quantified the effects of this noise on the confidence of estimating kinetic parameters of the underlying physiological responses and hence of model discrimination. Five subjects each performed eight transitions from 0 to 100 W on a cycle ergometer. Ventilation, CO2 output, and O2 uptake were computed breath by breath. The eight responses were interpolated uniformly, time aligned, and averaged for each subject; and the kinetic parameters of a first-order model (i.e., the time constant and time delay) were then estimated using three methods: a) linear least squares, b) nonlinear least squares, and c) maximum likelihood. The breath-by-breath noise approximated an uncorrelated Gaussian stochastic process, with a standard deviation that was largely independent of metabolic rate. An expression has therefore been derived for the number of square-wave repetitions required for a specified parameter confidence using methods b and c; method a being less appropriate for parameter estimation of noisy gas exchange kinetics.},
author = {Lamarra, N. and Whipp, B. J. and Ward, S. A. and Wasserman, K.},
doi = {10.1152/jappl.1987.62.5.2003},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lamarra et al. - 1987 - Effect of interbreath fluctuations on characterizing exercise gas exchange kinetics.pdf:pdf},
issn = {87507587},
journal = {Journal of Applied Physiology},
number = {5},
pages = {2003--2012},
pmid = {3110126},
title = {{Effect of interbreath fluctuations on characterizing exercise gas exchange kinetics}},
volume = {62},
year = {1987}
}
